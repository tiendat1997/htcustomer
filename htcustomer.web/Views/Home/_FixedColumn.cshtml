@model IEnumerable<htcustomer.service.ViewModel.Transaction.TransactionHomeViewModel>

<div class="panel panel-success transaction" id="fixed-column">
    <div class="panel-heading">
        <h3>@Html.ActionLink("Fixed", "Index", "Device", new { statusId = 2 }, null) <span class="badge">@Model.Count()</span></h3>
    </div>
    <div class="">
        <div class="table table-condensed table-striped list-container">
            <div class="list-group">
                @foreach (var transaction in Model)
                {
                    @Html.Partial("_FixedTransaction", transaction)   
                }
            </div>
        </div>
    </div>
</div>
