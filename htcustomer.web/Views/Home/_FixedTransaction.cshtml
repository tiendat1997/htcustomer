@model htcustomer.service.ViewModel.Transaction.TransactionHomeViewModel

<div class="list-group-item" style="position: relative">
    <button class="btn btn-sm btn-success btn-action"><i class="fa fa-send"></i></button>
    <h5 class="list-group-item-heading">
        <label class="label device-title">@Model.Category.Name</label>
        <a href="#" data-placement="bottom" data-toggle="tooltip" title="@Model.Customer.Phone">@Model.Customer.Name</a>
        <hr class="seperator-device" />
    </h5>
    <div class="list-group-item-text">
        <label class="label device-price">Price</label>

        <a data-toggle="collapse" data-target="#1-price"><b>@Model.Price,000 vnd</b></a>
        <div id="1-price" class="collapse">
            @Html.Partial("_DetailPriceTransaction", Model.ListPriceDetail)
        </div>
        <br />

        <label class="label device-description">Description</label>
        <ul>
            <li>
                <p>@Model.DeviceDescription</p>
            </li>
        </ul>

    </div>
</div>

