@model IEnumerable<htcustomer.service.ViewModel.Transaction.TransactionViewModel>

<div class="panel panel-warning">
    <div class="panel-heading" data-toggle="collapse" data-target="#notfix-panel" aria-expanded="false">
        Fixing &nbsp;<span class="badge">@Model.Count()</span>
    </div>
    <div class="panel-body" id="notfix-panel">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>Action</th>
                    <th>Device</th>
                    <th>Error</th>
                    <th>Receive</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var transaction in Model)
                {
                    @Html.Partial("_NotFixTransactionRow",transaction)
                }

            </tbody>
        </table>
    </div>
</div>

