@model Dictionary<string, IEnumerable<htcustomer.service.ViewModel.Transaction.TransactionViewModel>>

<div class="panel panel-success">
    <div class="panel-heading" data-toggle="collapse" data-target="#waiting-panel" aria-expanded="false">
        Waiting To Deliver &nbsp;<span class="badge">@(Model["FIXED"].Count() + Model["CANNOTFIX"].Count())</span>
    </div>
    <div class="panel-body" id="waiting-panel">
        <div style="margin-bottom: 50px;">
            <h4 style="border-bottom: 0.5px solid #ddd">Fixed Transaction</h4>
            <table class="table table-striped table-hover" id="fixed-table">
                <thead>
                    <tr>
                        <th>Action</th>
                        <th>Device</th>
                        <th>Error</th>
                        <th>Receive</th>
                        <th>Price</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var transaction in Model["FIXED"])
                    {
                        @Html.Partial("_FixedTransactionRow",transaction)
                    }

                </tbody>
            </table>
        </div>
        <div>
            <h4 style="border-bottom: 0.5px solid #ddd">Cannot Fix Transaction</h4>
            <table class="table table-striped table-hover" id="cannot-fix-table">
                <thead>
                    <tr>
                        <th>Action</th>
                        <th>Device</th>
                        <th>Error</th>
                        <th>Receive</th>
                        <th>Reason</th>
                    </tr>
                </thead>
                <tbody>

                    @foreach (var transaction in Model["CANNOTFIX"])
                    {
                        @Html.Partial("_CannotFixTransactionRow",transaction)
                    }

                </tbody>
            </table>
        </div>


    </div>
</div>

